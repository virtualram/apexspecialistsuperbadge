<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Name_cannot_be_changed</fullName>
    <active>true</active>
    <errorConditionFormula>IF(ISCHANGED( Name ), true,false)</errorConditionFormula>
    <errorMessage>Name cannot be changed</errorMessage>
</ValidationRule>
