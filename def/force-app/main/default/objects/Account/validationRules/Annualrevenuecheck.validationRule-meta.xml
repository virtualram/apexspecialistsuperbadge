<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Annualrevenuecheck</fullName>
    <active>true</active>
    <description>AND( NOT(ISNEW()))</description>
    <errorConditionFormula>AnnualRevenue &lt; 100</errorConditionFormula>
    <errorDisplayField>AnnualRevenue</errorDisplayField>
    <errorMessage>Annual revenue should be more than 200000</errorMessage>
</ValidationRule>
