<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Accountnamecannotbechanedineditmode</fullName>
    <active>false</active>
    <errorConditionFormula>IF(AND( ISCHANGED( Name ) ,NOT( ISNEW())),true,false)</errorConditionFormula>
    <errorMessage>Cannot change account name in edit mode</errorMessage>
</ValidationRule>
