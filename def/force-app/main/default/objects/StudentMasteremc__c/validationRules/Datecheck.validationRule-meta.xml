<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Datecheck</fullName>
    <active>true</active>
    <errorConditionFormula>DATETIMEVALUE(DOJ__c)  &lt;    DATETIMEVALUE(TODAY())</errorConditionFormula>
    <errorMessage>Date cannot be less than today</errorMessage>
</ValidationRule>
