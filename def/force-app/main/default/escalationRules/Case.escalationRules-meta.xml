    <escalationRule>
        <fullName>DellHopkinton100</fullName>
        <active>true</active>
        <ruleEntry>
            <businessHoursSource>Case</businessHoursSource>
            <criteriaItems>
                <field>Case.Origin</field>
                <operation>equals</operation>
                <value>Web,Phone1,Email</value>
            </criteriaItems>
            <escalationAction>
                <assignedTo>narenrr@yahoo.com</assignedTo>
